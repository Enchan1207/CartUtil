{
    "manifest_version": 2,
    "name": "秋月ウィッシュリスト",
    "version": "2.0.2",
    "description": "ウィッシュリストを作成、表示します。",
    "permissions": ["tabs"],
    "icons": {
        "48": "icon48.png"
    },
    "browser_action": {
        "default_popup": "popup/index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://akizukidenshi.com/*"
            ],
            "js": [
                "cscript/script.js"
            ]
        }
    ]
}